{
  "functions": {
    "src/index.js": {
      "runtime": "nodejs@18.18.0"
    }
  },
  "routes": [
    { "src": "/login", "dest": "/src/index.js" },
    { "src": "/signup", "dest": "/src/index.js" },
    { "src": "/sendauthcode", "dest": "/src/index.js" },
    { "src": "/verifyauthcode", "dest": "/src/index.js" },
    { "src": "/mfa/setup", "dest": "/src/index.js" },
    { "src": "/mfa/verify", "dest": "/src/index.js" },
    { "src": "/(.*)", "dest": "/src/index.js" }
  ]
}
